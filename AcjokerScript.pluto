    for gunners as gunner do
        local gunh = util.joaat(gunner)
        Streament(gunh)
    end

    local vhash = util.joaat(hash)
    Streament(vhash)
    Streament(drihs)

    for i= 1 , 100 do
        tar1.x = tar1.x + i * 2
        tar1.y = tar1.y + i * 2
   
        local vatkr = CREATE_VEHICLE(vhash, tar1.x, tar1.y, tar1.z, 0.0, true, true, false)
        Vmod(vatkr, 'fuckoff', 100, 40)
