function Vmod(vmod, plate, max, top)
    VEHICLE.SET_VEHICLE_FIXED(vmod)
    for Vehopts as v do
    local maxmods = VEHICLE.GET_NUM_VEHICLE_MODS(vmod, v[1] - 1)
        if maxmods > 0 then
            VEHICLE.SET_VEHICLE_MOD(vmod, v[1] - 1, maxmods - 1 , false)
        end
    end
    for Vehtogs as v do
    local maxmods = VEHICLE.GET_NUM_VEHICLE_MODS(vmod, v[1])
        if maxmods > 0 then
            VEHICLE.TOGGLE_VEHICLE_MOD(vmod, v[1] - 1, on)
        end
    end
    if Dlc(ENTITY.GET_ENTITY_MODEL(vmod)) then
        for Bennysopts as v do
            local maxmods = VEHICLE.GET_NUM_VEHICLE_MODS(vmod, v[1] - 1)
            if maxmods > 0 then
                VEHICLE.SET_VEHICLE_MOD(vmod, v[1] - 1, maxmods - 1, false)
            end
        end
    end
    Vextras(vmod)
    VEHICLE.SET_VEHICLE_NUMBER_PLATE_TEXT(vmod, plate)
    VEHICLE.SET_VEHICLE_MOD(vmod, 14, 0)
    VEHICLE.SET_VEHICLE_TYRE_SMOKE_COLOR(vmod, 0, 0, 0)
    VEHICLE.SET_VEHICLE_MAX_SPEED(vmod, max)
    VEHICLE.MODIFY_VEHICLE_TOP_SPEED(vmod, top)
    VEHICLE.SET_VEHICLE_BURNOUT(vmod, false)

end

