local function add_pet(name, hash, number, handle)
    local new_pet = {name=name, hash=hash, number=number, handle=handle}
    local pets_list = Load_settings(Pets_List)
    for pets_list as pet do
        if pet.number == new_pet.number then
            return true
        end
    end
    table.insert(pets_list, new_pet)
    Save_settings(Pets_List, pets_list)
    return true
end

local function remove_pet(name)
    local pets_list = Load_settings(Pets_List)
    for index, pet in pets_list do
        if pet.name == name then
            pets_list[index] = nil
            Save_settings(Pets_List, pets_list)
            return true
        end
    end
    return false
end

