            end

            if disbet >= too_far then
                xrtarget = false
            end

            if IS_PED_A_PLAYER(ped) and not xray.players then
                xrtarget = false
            end

            if not IS_PED_A_PLAYER(ped) and not xray.npcs then
                xrtarget = false
