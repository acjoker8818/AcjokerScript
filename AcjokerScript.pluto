function Vehatkr(invinc, invis, pid, hash)
    local targets = GET_PLAYER_PED_SCRIPT_INDEX(pid)
    local tar1 = GET_OFFSET_FROM_ENTITY_IN_WORLD_COORDS(targets, math.random(-1, 1), math.random(-1, 1), 0.0)
