            end
            DelEnt(spawatk)
            atkset.p = 0
            spawatk = {}
            util.yield(8000)
            while atkset.p < atkset.count and players.exists(pid) do
                local atkr = Atkrspawn(atkset.invinc, atkset.invis, pid , atkset.model)
                if atkset.clone then
